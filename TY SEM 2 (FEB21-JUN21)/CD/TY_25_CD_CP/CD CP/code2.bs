FUN concat(string1, string2) -> string1 + string2
FUN appendCool(string) -> string + " is cool!"
FUN print_table(number)
    FOR i = 1 TO 11 THEN
        PRINT(i*number)
    END
END
FUN map(element, func)
    VAR result = []
    FOR i = 0 TO LEN(element) THEN
        APPEND(result, func(element/i))
    END
    RETURN result
END
